{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmName": {
      "value": "simpleLinuxVM"
    },
    "adminUsername": {
      "value": "vagrant"
    },
    "authenticationType": {
      "value": "sshPublicKey"
    },
    "adminPasswordOrKey": {
      "value": "ssh-rsa  vagrant@ubuntu-bionic"
    },
    "dnsLabelPrefix": {
      "value": "[toLower(format('simplelinuxvm-{0}', uniqueString(resourceGroup().id)))]"
    },
    "ubuntuOSVersion": {
      "value": "18.04-LTS"
    },
	"location": {
      "value": "West Europe"
    },
    "vmSize": {
      "value": "Standard_B2s"
    },
    "virtualNetworkName": {
      "value": "vNet"
    },
    "subnetName": {
      "value": "Subnet"
    },
    "networkSecurityGroupName": {
      "value": "SecGroupNet"
    }
  }
}